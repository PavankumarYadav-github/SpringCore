<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspider.springcorexml.collections.Employee" name="employee1">
	
		<property name="name" value="captain"></property>
		
		<property name="phones">
			<list>
				<value>789456</value>
				<value>78945226</value>
				<null/>
			</list>
		</property>
		
		<property name="address">
			<set>
				<value>Pune</value>
				<value>Dubai</value>
				<value>Delhi</value>
			</set>
		</property>
		
		<property name="coures">
		<map>
		<entry key="Militry" value="6 months"></entry>
		<entry key="Shield Pratice" value="2 years"></entry>
		<entry key="Driving" value="3 months"></entry>
		</map>
		</property>
	</bean>

</beans>
