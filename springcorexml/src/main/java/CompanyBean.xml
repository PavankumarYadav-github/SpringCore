<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspider.springcorexml.bean.CompanyBean"
		name="company1">
		<property name="id" value="1" />
		<property name="name" value="Infosys" />
		<property name="location" value="Pune" />
		<property name="employees">
			<list>
				<bean class="com.jspider.springcorexml.bean.EmployeeBean"
					name="employee1">
					<property name="id" value="1"></property>
					<property name="name" value="Ramesh"></property>
					<property name="salary" value="20000"></property>
					
				</bean>
			</list>
		</property>
	</bean>

</beans>

